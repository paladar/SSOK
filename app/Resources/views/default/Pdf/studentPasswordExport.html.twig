<head>
    <meta charset="UTF-8"> 
</head>
<style>
    .col-sm-6{
        page-break-inside: avoid;
    }
</style>
{% for pass in passwords %}
    <div class="col-sm-6">
        <h4>Uczeń:</h4>
        <div class="row"><label><b>Login: </b></label>{{pass.0.0.username}}</div>
        <div class="row"><label><b>Hasło: </b></label>{{pass.0.0.password}}</div>
    </div>
    <div class="col-sm-6">
        <h4>Rodzic:</h4>
        <div class="row"><label><b>Login: </b></label>{{pass.1.0.username}}</div>
        <div class="row"><label><b>Hasło: </b></label>{{pass.1.0.password}}</div>
    </div>
    <hr>
{% endfor %}